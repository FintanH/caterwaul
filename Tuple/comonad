  λ(a : Type)
→ ./../Comonad/fromExtractExtend
  Type
  ./../Function/Type
  ./../Function/category
  (λ(b : Type) → ./Type { _1 = a, _2 = b })
  (λ(b : Type) → λ(tup : ./Type { _1 = a, _2 = b }) → tup._2)
  (   λ(c : Type)
    → λ(b : Type)
    → λ(f : ./Type { _1 = a, _2 = b } → c)
    → λ(tup : ./Type { _1 = a, _2 = b })
    → { _1 = tup._1, _2 = f tup }
  )
