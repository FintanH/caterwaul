    let kArrow = ./../../Function/Kind

in  let kProduct = ./../../Tuple/Kind

in    { arrow =
          ./../../Function/Type
      , additive =
          { identity = <>, op = ./../../Either/Type }
      , multiplicative =
          { identity = {}, op = ./../../Tuple/Type }
      }
    : ./../Rig/Kind kArrow kProduct Type
